{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "pos": {
      "root": "src/client",
      "sourceRoot": "src/client",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "outputPath": "dist/client",
            "index": "src/client/index.html",
            "main": "src/client/main.ts",
            "tsConfig": "src/client/tsconfig.app.json",
            "polyfills": "src/client/polyfills.ts",
            "assets": [
              "src/client/assets",
              "src/client/favicon.ico"
            ],
            "styles": [
              "node_modules/bootstrap-icons/font/bootstrap-icons.css",
              "node_modules/swiper/css/swiper.css",
              "node_modules/intl-tel-input/build/css/intlTelInput.css",
              "src/client/styles.scss"
            ],
            "scripts": [
              "src/client/assets/lib/star-web-print/StarWebPrintTrader.js",
              "src/client/assets/lib/star-web-print/StarWebPrintBuilder.js",
              "src/client/assets/lib/epson/epos-2.14.0.js",
              "node_modules/swiper/js/swiper.js",
              "src/client/assets/lib/ejs.min.js"
            ],
            "allowedCommonJsDependencies": [
              "hammerjs",
              "moment-timezone",
              "@cinerino/sdk",
              "qrcode",
              "jwt-decode",
              "platform"
            ]
          },
          "configurations": {
            "local": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "15kb"
                }
              ],
              "watch": true
            },
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "15kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": false,
              "statsJson": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "pos:build",
            "port": 3333,
            "ssl": true,
            "sslKey": "ssl/server.key",
            "sslCert": "ssl/server.crt"
          },
          "configurations": {
            "dev": {
              "browserTarget": "pos:build:dev"
            },
            "test": {
              "browserTarget": "pos:build:test"
            },
            "production": {
              "browserTarget": "pos:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "pos:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/client/test.ts",
            "karmaConfig": "src/client/karma.conf.js",
            "polyfills": "src/client/polyfills.ts",
            "tsConfig": "src/client/tsconfig.spec.json",
            "scripts": [
              "node_modules/swiper/dist/js/swiper.js"
            ],
            "styles": [
              "src/client/styles.scss",
              "node_modules/swiper/dist/css/swiper.css"
            ],
            "assets": [
              "src/client/assets",
              "src/client/favicon.ico"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "pos-e2e": {
      "root": "src/client",
      "sourceRoot": "src/client/e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "pos:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "pos",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "style": "scss",
      "skipTests": true
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  },
  "cli": {
    "warnings": {},
    "analytics": "78f8271a-b5f8-442e-9f70-3d2678e9386d"
  }
}