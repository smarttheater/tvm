    "scripts": {
        "dev": "npm-run-all -p dev:*",
        "dev:client": "ng build --configuration=local",
        "dev:server": "node dist/server/ssl",
        "ng": "ng",
        "build": "npm run build:client && npm run build:server",
        "build:client": "ng build --configuration=production",
        "build:server": "tsc --project src/server --outDir dist/server --locale ja-jp",
        "test:client": "ng test --code-coverage",
        "lint": "npm-run-all -p lint:*",
        "lint:client": "ng lint",
        "lint:server": "tslint --config tslint.json src/server/**/**.ts --exclude **/**.spec.ts --project src/server/tsconfig.json",
        "e2e": "ng e2e",
        "start": "node dist/server/index",
        "test": "npm run lint",
        "preversion": "npm run build",
        "version": "git add -A",
        "postversion": "git push origin --tags",
        "clean": "rimraf src/**/*",
        "analyze": "webpack-bundle-analyzer dist/client/stats.json",
        "deploy-development": "gcloud app deploy ./app.development.gcp.yaml --project smart-theater-development-tvm",
        "deploy-test": "gcloud app deploy ./app.test.gcp.yaml --project smart-theater-test-tvm",
        "deploy-production": "gcloud app deploy ./app.production.gcp.yaml --project smart-theater-production-tvm --no-promote --version staging"
    },
    "private": true,
